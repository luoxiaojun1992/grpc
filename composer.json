{
  "name": "luoxiaojun/grpc",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "coroutine",
    "grpc",
    "http",
    "http2",
    "client",
    "service",
    "generator"
  ],
  "description": "Grpc PHP Client base on Swoole Http2 Coroutine & Service Generator",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "luoxiaojun",
      "email": "luoxiaojun1992@sina.cn"
    }
  ],
  "support": {
    "issues": "https://github.com/swoole/etcd-client/issues"
  },
  "require": {
    "php": ">=7.1",
    "liues1992/php-protobuf-generator": "~0.1"
  },
  "autoload": {
    "classmap": [
      "src/Grpc"
    ],
    "files": [
      "src/include/functions.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "bin": ["bin/protoc-gen-grpc-php-plugin.php","tools/generator"]
}
